{
  "name": "node-inversify-mongoose-typescript-starter",
  "version": "1.0.0",
  "description": "A node.js project starter with Inversify express, mongoose, typescript, es-lint and basic auth flow.",
  "author": "Prajwal264",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon dist",
    "watch": "tsc -w",
    "build": "tsc --build tsconfig.json",
    "prepare": "husky install",
    "error": "eslint .",
    "fixlint": "eslint . --fix",
    "debug": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn src/index.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "nanoid": "^3.3.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
